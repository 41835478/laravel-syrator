{
    "name": "laravel-syrator",
    "description": "SYRATOR Framework based on Laravel.",
    "keywords": ["SYRATOR", "Base", "CMS", "framework", "Laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "erusev/parsedown-extra": "0.7.0",
        "syrator/identity-card": "~1.0",
        "jlapp/swaggervel": "dev-master",
        "stevenyangecho/laravel-u-editor": " ~1.1",
        "pingpong/modules": "~2.1",
        "overtrue/wechat": "~3.1",
        "xiaosier/libweibo": "dev-master",
        "orangehill/iseed": "2.2",
        "zizaco/entrust": "5.2.x-dev",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Syrator\\": "syrator/",
            "Modules\\": "modules/"
        },
        "files": [
            "app/helper.php",
            "syrator/helper.php",
            "vendor/qqconnect/API/qqConnectAPI.php",
            "vendor/qqsdk/OpenApiV3.php",
            "vendor/taobao/TopSdk.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
